<%= link_to 'New_Employe', new_employe_path, :class => "btn btn-primary" %>
<%= @employe.inspect %>

<div class="table-responsive">
<table id="example" class="table table-bordered table-hover">
  <thead>
  <tr>
    <th>Name</th>
    <th>Gender</th>
    <th>Mobile</th>
   </tr>
  </thead>

<tbody>
<% @employe.each do |employe| %>
  <tr>
    <td><%= employe.employe_name %></td>
    <td><%= employe.employe_gender %></td>
    <td><%= employe.employe_mob_no %></td>
    <td><%= link_to '<span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span>'.html_safe, employe %></td>
    <td><%= link_to '<span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>'.html_safe, edit_employe_path(employe) %></td>
    <td><%= link_to '<span class="glyphicon glyphicon-remove" aria-hidden="true"></span>'.html_safe, employe, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
 </tbody>
</table>
</div>
